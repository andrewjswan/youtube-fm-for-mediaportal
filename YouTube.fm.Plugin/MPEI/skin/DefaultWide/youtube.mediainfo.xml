<window>	
	<controls>
		<!-- Logos [ListView/AlbumView] -->
                <control>
                  <description>UnWatched BG Logo</description>
                  <type>image</type>
                  <id>0</id>
                  <posX>90</posX>
                  <posY>122</posY>
                  <width>28</width>
                  <height>28</height>
                  <keepaspectratio>yes</keepaspectratio>                  
                  <texture>overlayunwatched.png</texture>		  
                  <visible>[facadeview.list|facadeview.album]+!string.equals(#Youtube.fm.Curent.Video.Title,)+Control.HasFocus(50)+!string.equals(#selecteditem,..)</visible>                  	        
                </control> 
                <control>
                  <description>Watched Logo</description>
