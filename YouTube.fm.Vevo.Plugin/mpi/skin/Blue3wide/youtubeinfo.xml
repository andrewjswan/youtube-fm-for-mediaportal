<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
    <id>29052</id>
    <defaultcontrol>50</defaultcontrol>
    <allowoverlay>no</allowoverlay>
    <define>#header.hover:hover_musicvideo.png</define>
    <define>#header.title:YouTube Videos</define>
    <define>#header.label:YouTube Videos</define>
    <define>#header.image:-</define>
    <controls>
        <import>common.window.video.xml</import>
        <control>
            <id>95</id>
            <description>Fan Art</description>
            <type>image</type>
            <posX>-86</posX>
            <posY>-48</posY>
            <width>1366</width>
            <height>768</height>
            <texture>#Youtube.fm.NowPlaying.Video.FanArt</texture>
        </control>
        <import>common.time.xml</import>
        <control>
            <id>101</id>
            <description>Selected item Label</description>
            <type>fadelabel</type>
            <posX>1173</posX>
            <posY>470</posY>
            <width>711</width>
            <font>font14</font>
            <textcolor>ffffffff</textcolor>
            <align>RIGHT</align>
            <label>#selecteditem</label>
            <animation effect="Fade" time="250">WindowOpen</animation>
            <animation effect="Fade" time="250">WindowClose</animation>
        </control>
        <control>
            <id>1</id>
            <type>imagelist</type>
            <posX>680</posX>
            <posY>84</posY>
            <width>120</width>
            <height>24</height>
            <textureHeight>24</textureHeight>
            <textureWidth>24</textureWidth>
            <percentage>#Youtube.fm.NowPlaying.Video.Rating</percentage>
            <imagesToDraw>5</imagesToDraw>
            <orientation>HORIZONTAL</orientation>
            <align>RIGHT</align>
            <subitems>
                <subitem>greystar.png</subitem>
                <subitem>starbest.png</subitem>
            </subitems>
        </control>
        <control>
            <id>1</id>
            <description>Video Image</description>
            <type>videowindow</type>
            <visible>Player.HasVideo</visible>
            <posX>50</posX>
            <posY>80</posY>
            <width>480</width>
            <height>324</height>
            <action>18</action>
            <textureFocus>tv_green_border.png</textureFocus>
            <keepaspectratio>yes</keepaspectratio>
        </control>
        <control>
            <id>47</id>
            <description>meta label</description>
            <type>label</type>
            <posX>71</posX>
            <posY>32</posY>
            <font>font12</font>
            <textcolor>ffffffff</textcolor>
            <align>LEFT</align>
            <label>#Youtube.fm.NowPlaying.Video.Title</label>
        </control>
        <control>
            <id>47</id>
            <description>meta label</description>
            <type>label</type>
            <posX>71</posX>
            <posY>52</posY>
            <font>font12</font>
            <textcolor>ffffffff</textcolor>
            <align>LEFT</align>
            <label>#Youtube.fm.NowPlaying.Artist.Name</label>
        </control>
        <control>
            <id>50</id>
            <description>similar videos</description>
            <type>thumbnailpanel</type>
            <onleft>1</onleft>
            <onright>1</onright>
            <posX>-20</posX>
            <posY>502</posY>
            <width>1300</width>
            <height>218</height>
            <spinWidth>24</spinWidth>
            <spinHeight>24</spinHeight>
            <spinPosX>60</spinPosX>
            <spinPosY>687</spinPosY>
            <spinAlign>Right</spinAlign>
            <scrollOffset>1</scrollOffset>
            <unfocusedAlpha>120</unfocusedAlpha>
            <imageFolderFocus>thumbborder.png</imageFolderFocus>
            <itemWidth>185</itemWidth>
            <itemHeight>200</itemHeight>
            <textureWidth>170</textureWidth>
            <textureHeight>170</textureHeight>
            <thumbWidth>156</thumbWidth>
            <thumbHeight>156</thumbHeight>
            <thumbPosX>7</thumbPosX>
            <thumbPosY>7</thumbPosY>
            <itemWidthBig>185</itemWidthBig>
            <itemHeightBig>200</itemHeightBig>
            <textureWidthBig>170</textureWidthBig>
            <textureHeightBig>170</textureHeightBig>
            <thumbWidthBig>156</thumbWidthBig>
            <thumbHeightBig>156</thumbHeightBig>
            <thumbPosXBig>7</thumbPosXBig>
            <thumbPosYBig>7</thumbPosYBig>
            <thumbZoom>yes</thumbZoom>
            <enableFocusZoom>yes</enableFocusZoom>
            <textureUp>page_up_nofocus.png</textureUp>
            <textureDown>page_down_nofocus.png</textureDown>
            <textureUpFocus>page_up_focus.png</textureUpFocus>
            <textureDownFocus>page_down_focus.png</textureDownFocus>
        </control>
        <control>
            <type>label</type>
            <posX>561</posX>
            <posY>121</posY>
            <label>Duration:</label>
        </control>
        <control>
            <type>label</type>
            <posX>715</posX>
            <posY>120</posY>
            <label>#Youtube.fm.NowPlaying.Video.Duration</label>
        </control>
        <control>
            <type>label</type>
            <posX>561</posX>
            <posY>152</posY>
            <label>Author:</label>
        </control>
        <control>
            <type>label</type>
            <posX>562</posX>
            <posY>185</posY>
            <label>Views:</label>
        </control>
        <control>
            <type>label</type>
            <posX>714</posX>
            <posY>151</posY>
            <label>#Youtube.fm.NowPlaying.Video.Autor</label>
        </control>
        <control>
            <type>label</type>
            <posX>714</posX>
            <posY>183</posY>
            <label>#Youtube.fm.NowPlaying.Video.ViewCount</label>
        </control>
        <control>
            <type>label</type>
            <posX>562</posX>
            <posY>219</posY>
            <label>Published:</label>
        </control>
        <control>
            <type>label</type>
            <posX>713</posX>
            <posY>217</posY>
            <label>#Youtube.fm.NowPlaying.Video.PublishDate</label>
        </control>
        <control>
            <type>label</type>
            <posX>560</posX>
            <posY>250</posY>
            <label>Favorites</label>
        </control>
        <control>
            <type>label</type>
            <posX>712</posX>
            <posY>250</posY>
            <label>#Youtube.fm.NowPlaying.Video.FavoriteCount</label>
        </control>
    </controls>
</window>
